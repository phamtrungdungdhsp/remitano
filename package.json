{
  "name": "restaurant-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.json --testTimeout=60000 --forceExit --detectOpenHandles --passWithNoTests",
    "test:unit": "jest --config jest-unit.json --testTimeout=60000 --forceExit --detectOpenHandles",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "yarn typeorm migration:run -- -d ./src/config/orm.config.ts",
    "migration:create": "yarn typeorm migration:create",
    "migration:revert": "yarn typeorm migration:revert -- -d ./src/config/orm.config.ts",
    "seed:config": "typeorm-seeding config -c node_modules/@concepta/typeorm-seeding/dist/seeding-source.js",
    "seed:run": "typeorm-seeding seed -c node_modules/@concepta/typeorm-seeding/dist/seeding-source.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^6.1.0",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/typeorm": "^9.0.0-next.2",
    "@types/cache-manager": "^4.0.1",
    "bcrypt": "^5.0.1",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nestjs-typeorm-paginate": "^4.0.2",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.5.0",
    "ts-morph": "^15.1.0",
    "typeorm": "^0.3.7",
    "ytdl-core": "^4.11.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "superagent": "^8.0.0",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
